        			android:layout_height="wrap_content"  android:layout_marginRight="0dip"
        			android:layout_marginTop="10dip"
    				android:background="@drawable/check_blue"
	    />
	</RelativeLayout>   	   	       


    <RelativeLayout android:id="@+id/re_france"
    	android:layout_below="@+id/label_about_your_honeymoon_subtitle"
    	android:layout_width="wrap_content" android:layout_toRightOf="@+id/re_antigua"
    	android:layout_height="wrap_content">
    
	       <TextView android:id="@+id/label_france"
	        android:layout_below="@+id/re_british"
	    	android:text="@string/label_france"  android:layout_toRightOf="@+id/check_blue1"
