    	android:textStyle="bold" android:layout_marginTop="4dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="100dip" android:textColor="@color/black"
    	/>
    	
    	<ImageView android:id="@+id/check_blue1" android:visibility="invisible"
    	           android:layout_below="@+id/label_about_your_honeymoon_subtitle" android:layout_toRightOf="@+id/label_antigua"
    			   android:layout_width="wrap_content" android:layout_marginLeft="0dip"
        			android:layout_height="wrap_content"  android:layout_marginRight="0dip"
        			android:layout_marginTop="10dip"
    				android:background="@drawable/check_blue"
	    />
	</RelativeLayout>     	    
	
    <RelativeLayout android:id="@+id/re_bahamas"
    	android:layout_below="@+id/re_antigua"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content">
    
		 <TextView android:id="@+id/label_bahamas"
		        android:layout_below="@+id/re_antigua"
		    	android:text="@string/label_bahamas"
		    	android:textSize="18sp" android:layout_marginLeft="20dip"
		    	android:textStyle="bold" android:layout_marginTop="2dip"
		    	android:layout_height="wrap_content" 
		    	android:layout_width="100dip" android:textColor="@color/black"
