	    	android:textStyle="bold" android:layout_marginTop="2dip"
	    	android:layout_height="wrap_content" 
	    	android:layout_width="100dip" android:textColor="@color/black"
	    	/>       	    
   	 	
		    	
    	<ImageView android:id="@+id/check_blue5" android:visibility="invisible" 
    	           android:layout_below="@+id/re_british" android:layout_toRightOf="@+id/label_france"
    			   android:layout_width="wrap_content" android:layout_marginLeft="0dip"
        			android:layout_height="wrap_content"  android:layout_marginRight="0dip"
        			android:layout_marginTop="10dip"
    				android:background="@drawable/check_blue"
	    />
	</RelativeLayout>   	 
    	    
    	    
    <RelativeLayout android:id="@+id/re_hawaii"
    	android:layout_below="@+id/re_france"  android:layout_toRightOf="@+id/re_bahamas"
