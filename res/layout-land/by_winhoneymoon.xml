<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent" android:background="@drawable/background_0"
    android:layout_height="fill_parent">


<RelativeLayout android:id="@+id/first_table"
    android:orientation="vertical"
    android:layout_width="450dip" 
    android:layout_height="fill_parent">

	<ImageView android:id="@+id/titleBar"
    	android:layout_width="fill_parent"
        android:layout_height="43dp"
    	android:background="@drawable/title_bar"
    />
     
     <TextView 
    	android:text="@string/honeymoon_title"
    	android:textSize="22sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="5dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="wrap_content" 
    	android:layout_alignParentTop="true" 
    	android:layout_centerHorizontal="true"/>
    	

     <TextView android:id="@+id/title_1"
        android:layout_below="@+id/titleBar"
    	android:text="@string/honeymoon_title"
    	android:textSize="22sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="5dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="fill_parent" android:textColor="@color/black"
    	/>
    	
     <TextView android:id="@+id/title_2" 
     	android:layout_below="@+id/title_1"
    	android:text="@string/win_a_honeymoon_subitile"
    	android:textSize="14sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="5dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="wrap_content" android:textColor="@color/black"
     />

	<ImageView android:id="@+id/image1" android:layout_below="@+id/title_2"
    	android:layout_width="wrap_content" android:layout_marginLeft="10dip"
        android:layout_height="wrap_content"  android:layout_marginRight="10dip"
    	android:background="@drawable/superclubswin"
    />
    
     <TextView android:id="@+id/title_3" 
     	android:layout_below="@+id/image1"
    	android:text="@string/win_a_honeymoon_subitile2"
    	android:textSize="16sp" android:layout_marginLeft="15dip"
    	android:textStyle="bold" android:layout_marginTop="5dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="wrap_content" android:textColor="@color/black"
     />    

     <Button android:id="@+id/rules_regulation"  android:layout_below="@+id/title_3"
     	android:text="@string/rules_regulations" 
     	android:layout_width="fill_parent" 
     	android:layout_height="wrap_content" 
     	android:textSize="20sp" 
     	android:textStyle="bold"  android:layout_marginLeft="20dip" android:layout_marginRight="20dip"
     	android:textColor="@color/black"
     	android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="2dp"
        android:paddingBottom="2dp"
    	android:background="@drawable/xml_white"
     	android:layout_marginTop="30dp"  android:gravity="center_horizontal"
     	>
     	
     </Button>
</RelativeLayout>

    
<RelativeLayout android:id="@+id/title_table"
    android:layout_toRightOf="@+id/first_table"
    android:orientation="vertical"
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content">
         
       <TextView android:id="@+id/title_ta" 
    	android:text="Please complete all fields below to qualify your entry:"
    	android:textSize="14sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="10dip" android:layout_marginBottom="10dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="fill_parent" android:textColor="@color/red"
     />
       
</RelativeLayout>         

<RelativeLayout android:id="@+id/second_table"
    android:layout_toRightOf="@+id/first_table"  android:layout_below="@+id/title_table"
    android:orientation="vertical"
    android:layout_width="415dip" 
    android:layout_height="fill_parent">
        	
    <View android:id="@+id/divider" 
    android:layout_width="5dip"
    android:layout_height="fill_parent" 
    android:layout_toRightOf="@id/master"
    android:background="#ccc"
    android:layout_marginTop="43dp"/>    	
    
     <TextView android:id="@+id/title_2" 
     	android:layout_below="@+id/title_1"  android:visibility="gone"
    	android:text="@string/label_contact_information"
    	android:textSize="20sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="5dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="fill_parent" android:textColor="@color/black"
     />    
   
    <TextView android:id="@+id/req_et_name" 
     	android:layout_below="@+id/title_table"
    	android:text="First\nName:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="15dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="#000000"
     />    
   
   <EditText android:id="@+id/et_name" android:layout_below="@+id/title_2" android:layout_toRightOf="@+id/req_et_name"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip"
			android:layout_marginRight="15dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip" android:width="150dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_username"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 		    
	
    <TextView android:id="@+id/req_et_lastname" android:layout_toRightOf="@+id/et_name"
     	android:layout_below="@+id/title_2"
    	android:text="Last\nName:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="5dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="#000000"
     />    
     	
   <EditText android:id="@+id/et_lastname" android:layout_below="@+id/title_2" android:layout_toRightOf="@+id/req_et_lastname"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip" android:width="150dip"
			android:layout_marginRight="15dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_lastname"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 	
	
    <TextView android:id="@+id/req_et_email" 
     	android:layout_below="@+id/req_et_lastname"
    	android:text="Email:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="15dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="#000000"
     />   	
	
   <EditText android:id="@+id/et_email" android:layout_below="@+id/et_lastname" android:layout_toRightOf="@+id/req_et_email"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip"
			android:layout_marginRight="15dip" android:width="270dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_email"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 	
	
    <TextView android:id="@+id/req_et_address1" 
     	android:layout_below="@+id/req_et_email"  android:visibility="gone"
    	android:text="*"  android:layout_height="wrap_content" 
    	android:textSize="21sp" android:layout_marginLeft="15dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="@color/red"
     />   		
	
   <EditText android:id="@+id/et_address1" android:layout_below="@+id/et_email" android:layout_toRightOf="@+id/req_et_address1"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip" android:visibility="gone"
			android:layout_marginRight="15dip" android:width="250dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_address1"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 			    	
    		
    		
   <EditText android:id="@+id/et_address2" android:layout_below="@+id/et_address1"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip" android:visibility="gone"
			android:layout_marginRight="15dip" android:width="250dip"
			android:layout_marginLeft="35dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_address2"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 	
	
	
    <TextView android:id="@+id/req_et_city" 
     	android:layout_below="@+id/et_address2"
    	android:text="City:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="15dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="#000000"
     />   		
	
	<EditText android:id="@+id/et_city" android:layout_below="@+id/et_address2" android:layout_toRightOf="@+id/req_et_city"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip"
			android:layout_marginRight="15dip" android:width="130dip"
			android:layout_marginLeft="20dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000"
			android:hint="@string/hint_city"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/>
	
	  <TextView android:id="@+id/req_et_state"  android:layout_toRightOf="@+id/et_city"
     	android:layout_below="@+id/et_address2"
    	android:text="State:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="3dip"
    	android:layout_marginTop="15dip" 
    	android:layout_width="wrap_content" android:textColor="#000000"
     />   		
	
	
	<EditText android:id="@+id/et_state" android:layout_below="@+id/et_address2" android:layout_toRightOf="@+id/req_et_state"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip"
			android:layout_marginRight="15dip" android:width="50dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True" 
			android:textColor="#000000"
			android:hint="@string/hint_state"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 	  
	
	<TextView android:id="@+id/req_et_zip" android:layout_toRightOf="@+id/et_state"
     	android:layout_below="@+id/et_address2"
    	android:text="Zip:"  android:layout_height="wrap_content" 
    	android:textSize="14sp" android:layout_marginLeft="3dip"
    	android:layout_marginTop="15dip"
    	android:layout_width="wrap_content" android:textColor="#000000"
     />   	
	
	
	<EditText android:id="@+id/et_zip" android:layout_below="@+id/et_address2" android:layout_toRightOf="@+id/req_et_zip"
			android:layout_width="wrap_content" 
			android:layout_marginTop="10dip"
			android:layout_marginRight="15dip" android:width="50dip"
			android:layout_marginLeft="10dip"
			android:layout_height="35dip"
			android:textSize="12sp" 
			android:singleLine="True"
			android:textColor="#000000" android:maxLength="5"
			android:hint="@string/hint_zip"
			android:paddingLeft="6dip"
			android:background="@drawable/xml_white"
	/> 	  	 	    			    	
    
       <TextView android:id="@+id/label_about_your_honeymoon"
        android:layout_below="@+id/req_et_state"
    	android:text="@string/label_about_your_honeymoon"
    	android:textSize="20sp" android:layout_marginLeft="10dip"
    	android:textStyle="bold" android:layout_marginTop="30dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="fill_parent" android:textColor="@color/black"
    	/>
    	
       <TextView android:id="@+id/label_about_your_honeymoon_subtitle"
        android:layout_below="@+id/label_about_your_honeymoon"
    	android:text="@string/label_about_your_honeymoon_subtitle"
    	android:textSize="12sp" android:layout_marginLeft="10dip"
    	android:layout_marginTop="3dip"
    	android:layout_height="wrap_content" android:gravity="center_horizontal"
    	android:layout_width="fill_parent" android:textColor="@color/black"
    	/>    	
    
    <RelativeLayout android:id="@+id/re_antigua"
    	android:layout_below="@+id/label_about_your_honeymoon_subtitle"
