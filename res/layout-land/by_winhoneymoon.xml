    	android:layout_width="100dip" android:textColor="@color/black"
    	/>       	   	    
   	 	
		    	
    	<ImageView android:id="@+id/check_blue7" android:visibility="invisible" 
    	           android:layout_below="@+id/re_hawaii" android:layout_toRightOf="@+id/label_italy"
    			   android:layout_width="wrap_content" android:layout_marginLeft="0dip"
        			android:layout_height="wrap_content"  android:layout_marginRight="0dip"
        			android:layout_marginTop="10dip"
    				android:background="@drawable/check_blue"
	    />
	</RelativeLayout>      	       	    


   	
    <RelativeLayout android:id="@+id/re_jamaica" android:layout_toRightOf="@+id/re_british"
    	android:layout_below="@+id/re_italy"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content">
    
	 <TextView android:id="@+id/label_jamaica"
        android:layout_below="@+id/re_italy"
    	android:text="@string/label_jamaica"
    	android:textSize="18sp" android:layout_marginLeft="20dip"
    	android:textStyle="bold" android:layout_marginTop="2dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="100dip" android:textColor="@color/black"
    	/>      	   	    
   	 	
		    	
    	<ImageView android:id="@+id/check_blue8" android:visibility="invisible" 
    	           android:layout_below="@+id/re_italy" android:layout_toRightOf="@+id/label_jamaica"
    			   android:layout_width="wrap_content" android:layout_marginLeft="0dip"
        			android:layout_height="wrap_content"  android:layout_marginRight="0dip"
        			android:layout_marginTop="10dip"
    				android:background="@drawable/check_blue"
	    />
	</RelativeLayout> 

    
	
	
   <RelativeLayout android:id="@+id/re_stbarts" android:layout_toRightOf="@+id/re_france"
    	android:layout_below="@+id/label_about_your_honeymoon_subtitle"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content">
    
    	<TextView android:id="@+id/label_stbarts"
        android:layout_below="@+id/re_jamaica"
    	android:text="@string/label_sbarts"
    	android:textSize="18sp" android:layout_marginLeft="20dip"
    	android:textStyle="bold" android:layout_marginTop="2dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="100dip" android:textColor="@color/black"
    	/>     
   	 	
		    	
    	<ImageView android:id="@+id/check_blue9" android:visibility="invisible" 
