    	<TextView android:id="@+id/label_stlucia"
        android:layout_below="@+id/re_stbarts"
    	android:text="@string/label_slucia"
    	android:textSize="18sp" android:layout_marginLeft="20dip"
    	android:textStyle="bold" android:layout_marginTop="2dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="100dip" android:textColor="@color/black"
    	/>     
   	 	
		    	
    	<ImageView android:id="@+id/check_blue10" android:visibility="invisible" 
    	           android:layout_below="@+id/re_stbarts" android:layout_toRightOf="@+id/label_stlucia"
