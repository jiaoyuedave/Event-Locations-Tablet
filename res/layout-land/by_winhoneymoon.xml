       <TextView android:id="@+id/label_hawaii"
        android:layout_below="@+id/re_hawaii"
    	android:text="@string/label_hawaii"
    	android:textSize="18sp" android:layout_marginLeft="20dip"
    	android:textStyle="bold" android:layout_marginTop="2dip"
    	android:layout_height="wrap_content" 
    	android:layout_width="100dip" android:textColor="@color/black"
    	/>       	   	    
   	 	
		    	
    	<ImageView android:id="@+id/check_blue6" android:visibility="invisible" 
    	           android:layout_below="@+id/re_hawaii" android:layout_toRightOf="@+id/label_hawaii"
